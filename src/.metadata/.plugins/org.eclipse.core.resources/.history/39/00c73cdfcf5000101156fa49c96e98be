package modelo;

public class Circulo extends Figura{

	private double radio;
	
	public Circulo(double x, double y, double radio) {
		super(x, y);
		this.radio = radio;
		}
	
	
	
	@Override
	public boolean equals(Oblect otro) {
		if(!super.equals(otro)) return false;
		Circulo c = (Circulo)otro;
		return this.
		
	}
	
}
